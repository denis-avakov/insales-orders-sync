{
  "compilerOptions": {
    "baseUrl": "./",
    "module": "esnext",
    "moduleResolution": "node",
    "paths": {
      "@redwoodjs/testing": ["../node_modules/@redwoodjs/testing/api"],
      "src/*": ["../.redwood/types/mirror/api/src/*", "./src/*"],
      "types/*": ["../types/*", "./types/*"]
    },
    "rootDirs": ["../.redwood/types/mirror/api/src", "./src"],
    "typeRoots": ["../node_modules/@types", "./node_modules/@types"],
    "types": ["jest"],
    "noEmit": true,
    "allowJs": true,
    "esModuleInterop": true,
    "target": "esnext"
  },
  "include": [
    "src",
    "../.redwood/types/includes/all-*",
    "../.redwood/types/includes/api-*",
    "../types"
  ]
}
